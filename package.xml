<?xml version="1.0"?>
<package format="2">
  <name>lidar_cones_detection</name>
  <version>0.0.0</version>
  <description>Package to handle the identification of cones using LiDAR data.</description>

  <maintainer email="caleb@todo.todo">caleb</maintainer>
  <maintainer email="riley.d.bowyer@gmail.com">Riley Bowyer</maintainer>


  <license>MIT</license>

  <!-- Catkin -->
  <buildtool_depend>catkin</buildtool_depend>

  <!-- PCL -->
  <depend>pcl_ros</depend>
  <depend>pcl_conversions</depend>
  <depend>PCL</depend>

  <!-- Eigen -->
  <depend>Eigen3</depend>

  <!-- ROS Packages -->
  <depend>roscpp</depend>
  <depend>tf2</depend>
  <depend>cv_bridge</depend>

  <!-- Messages -->
  <depend>sensor_msgs</depend>

</package>
